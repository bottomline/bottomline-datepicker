  "description": "A Glu DatePicker for React applications",
  "main": "umd/index.js",
  "module": "esm/index.js",
  "scripts": {
    "lint": "eslint --fix --ignore-path ./.gitignore --ext .js,.jsx .",
    "test": "jest",
    "build": "npm run clean && npm run copy && rollup -c && npm run babel && npm run babelCJS",
    "devPackage": "npm run lint && npm run test && npm run build && cd dist && npm pack",
    "clean": "shx rm -rf dist",
    "copy": "shx mkdir dist && shx cp -r src dist/src && shx cp {package.json,README.md,.npmignore} dist",
    "babel": "babel src --out-dir dist/lib",
    "babelCJS": "babel src --presets=@babel/preset-env,@babel/preset-react --out-dir dist/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.saas-p.com:7999/glu/glu-datepicker-react.git"
